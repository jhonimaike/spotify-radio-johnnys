  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "protocol": "inspector",
      "restart": true,
      "name": "Debug",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ]
    },
    {
      "name": "Launch Edge",
      "request": "launch",
      "type": "pwa-msedge",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}"
    },
    // {
    //   "type": "pwa-chrome",
    //   "request": "launch",
    //   "name": "Launch Chrome against localhost",
    //   "url": "http://localhost:8080",
    //   "webRoot": "${workspaceFolder}"
    // }
  ]
}